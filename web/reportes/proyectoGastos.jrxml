	<queryString language="SQL">
		<![CDATA[SELECT p.idProyecto AS Codigo, p.nombreProyecto AS Proyecto, COUNT(e.dui) as N°_Empleados ,SUM(e.salario) as Gasto, p.estadoProyecto AS Estado, d.nombreDepto as Deptartamento FROM proyecto AS p INNER JOIN detalleempleado AS de ON (de.proyectoId = p.idProyecto) INNER JOIN empleado as e ON (e.dui = de.empleadoDUI) INNER JOIN departamento AS d ON (d.idDepto = p.deptoId);]]>
