    <servlet>
        <servlet-name>EmpleadoServlet</servlet-name>
        <servlet-class>com.controlador.EmpleadoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DEServlet</servlet-name>
        <servlet-class>com.controlador.DEServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MunServlet</servlet-name>
        <servlet-class>com.controlador.MunServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeptoServlet</servlet-name>
        <servlet-class>com.controlador.DeptoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProyectoServlet</servlet-name>
        <servlet-class>com.controlador.ProyectoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MaqServlet</servlet-name>
        <url-pattern>/MaqServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DMServlet</servlet-name>
        <url-pattern>/DMServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fotoPerfil</servlet-name>
        <url-pattern>/fotoPerfil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/UsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmpleadoServlet</servlet-name>
        <url-pattern>/EmpleadoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DEServlet</servlet-name>
        <url-pattern>/DEServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MunServlet</servlet-name>
        <url-pattern>/MunServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeptoServlet</servlet-name>
        <url-pattern>/DeptoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProyectoServlet</servlet-name>
        <url-pattern>/ProyectoServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
